{
	"name": "@shadowvzs/react-view-router",
	"version": "0.8.2",
	"description": "Router for ReactJS with MobX+Typescript, decoupled component and logic with introducing the viewStore layer between routes and component which can instantiate outside as well and can inject data for all instantiated viewStore",
	"repository": {
		"type": "git",
		"url": "https://github.com/shadowvzs/react-view-router.git"
	},
	"main": "./dist/cjs/index.js",
	"module": "./dist/esm/index.js",
	"types": "./dist/esm/index.d.ts",
	"keywords": [
		"react",
		"typescript",
		"mobx",
		"router",
		"store",
		"view",
		"viewStore"
	],
	"scripts": {
		"clean": "rimraf ./dist",
		"build": "npm run clean && yarn build:esm && yarn build:cjs",
		"build:esm": "tsc",
		"build:cjs": "tsc --module commonjs --outDir dist/cjs",
		"build2": "npm run clean && webpack --config webpack.config.js --progress",
		"lint": "eslint \"{**/*,*}.{js,ts,jsx,tsx}\"",
		"prettier": "prettier --write \"{src,tests,example/src}/**/*.{js,ts,jsx,tsx}\"",
		"test": "jest ./test",
		"start": "webpack-dev-server --mode development",
		"prepare": "npm run build",
		"prepublishOnly": "npm test && npm run prettier && npm run lint"
	},
	"peerDependencies": {
		"mobx": ">=6",
		"mobx-react-lite": ">=3",
		"react": ">=18",
		"react-dom": ">=18"
	},
	"author": "Varga Zsolt (shadowvzs)",
	"license": "MIT",
	"devDependencies": {
		"@babel/plugin-transform-modules-commonjs": "^7.19.6",
		"@babel/preset-env": "^7.20.2",
		"@babel/preset-react": "^7.18.6",
		"@jest/globals": "^29.3.1",
		"@testing-library/react": "^13.4.0",
		"@types/jest": "^29.2.3",
		"@types/node": "^18.11.9",
		"@types/react": "^18.0.25",
		"@types/react-dom": "^18.0.8",
		"@typescript-eslint/eslint-plugin": "^5.43.0",
		"@typescript-eslint/parser": "^5.43.0",
		"babel-jest": "^29.3.1",
		"babel-loader": "^9.1.0",
		"cross-env": "^7.0.3",
		"css-loader": "^6.7.2",
		"eslint": "^8.27.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.31.10",
		"html-webpack-plugin": "^5.5.0",
		"jest": "^29.3.1",
		"jest-environment-jsdom": "^29.3.1",
		"mini-css-extract-plugin": "^2.6.1",
		"mobx": "^6.6.2",
		"mobx-react-lite": "^3.4.0",
		"prettier": "^2.7.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-test-renderer": "^18.2.0",
		"rimraf": "^3.0.2",
		"style-loader": "^3.3.1",
		"terser-webpack-plugin": "^5.3.6",
		"ts-jest": "^29.0.3",
		"ts-loader": "^9.4.1",
		"typescript": "^4.8.4",
		"typescript-eslint-language-service": "^5.0.0",
		"webpack": "^5.75.0",
		"webpack-cli": "^4.10.0",
		"webpack-dev-server": "^4.11.1",
		"yarn": "^1.22.19"
	},
	"files": [
		"dist",
		"LICENSE",
		"README.md"
	]
}
